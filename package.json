  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/QuantStack/jupyterlab-debugger",
  "bugs": {
    "url": "https://github.com/QuantStack/jupyterlab-debugger/issues"
